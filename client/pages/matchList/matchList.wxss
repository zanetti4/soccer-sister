.date {
  text-align: center;
  background-color: #F4F4F4;
  line-height: 2.5;
  display: block;
}
.top-con {
  display: flex;
  text-align: center;
  font-size: 22rpx;
  padding: 20rpx 0;
  border-bottom: solid 1px #D6D6D6;
}
.top-goal {
  background: #FBF4CE url(data:image/png;base64,) no-repeat 80rpx 80rpx;
}
.top-con-team {
  flex: 1 0 0;
}
.top-con-team image {
  display: inline-block;
  width: 80rpx;
  height: 80rpx;
}
.top-con-cen {
  flex: 2 0 0;
  line-height: 1.8;
}
.top-con-cen-tit {
  color: #999;
}
.top-con-cen-line {
  height: 1px;
  /* for w3c */
  background-image: linear-gradient(left, transparent 0%, rgba(0, 0, 0, 0.3) 50%, transparent 100%);
  /* for chrome 10+, safari 5.1+ */
  background-image: -webkit-linear-gradient(left, transparent 0%, rgba(0, 0, 0, 0.3) 50%, transparent 100%);
}
.centerTxt {
  font-size: 26rpx;
}
.top-con-cen-scores {
  font-size: 26rpx;
  display: flex;
}
.top-con-cen-lives {
  color: #F2708C;
  font-size: 40rpx;
  line-height: 2.2;
}
.top-con-cen-scores-num {
  flex: 6 0 0;
}
.top-con-cen-scores-colon {
  flex: 1 0 0;
}
.sound {
  text-align: center;
  background-color: white;
  position: fixed;
  z-index: 100;
  top: 0;
  left: 0;
  width: 750rpx;
  padding: 10rpx 0;
}
.sound switch {
  zoom: 0.6;
  margin-left: 20rpx;
  vertical-align: middle;
}
